{
  "endpoints": [
    {
      "id": "primary",
      "name": "Primary Endpoint",
      "baseUrl": "<env.AZURE_API_BASE_URL>",
      "apiVersion": "2025-04-01-preview",
      "deployments": [
        {
          "id": "dalle-3",
          "name": "DALL-E 3",
          "deploymentName": "dalle-3",
          "maxSize": "1024x1024",
          "supportedFormats": ["png", "jpeg"],
          "description": "High-quality image generation with DALL-E 3"
        },
        {
          "id": "flux-1-1-pro",
          "name": "FLUX 1.1 Pro",
          "deploymentName": "FLUX-1.1-pro",
          "maxSize": "1024x1024",
          "supportedFormats": ["png", "jpeg"],
          "description": "High-quality image generation with FLUX 1.1 Pro model"
        },
        {
          "id": "FLUX.1-Kontext-pro",
          "name": "FLUX 1 Kontext Pro",
          "deploymentName": "FLUX.1-Kontext-pro",
          "maxSize": "1024x1024",
          "supportedFormats": ["png", "jpeg"],
          "description": "High-quality image generation with FLUX 1 Kontext Pro model"
        }
      ]
    }
  ],
  "defaultSettings": {
    "outputFormat": "png",
    "size": "1024x1024",
    "n": 1
  },
  "ui": {
    "theme": "light",
    "showConsole": true,
    "animationsEnabled": true
  }
